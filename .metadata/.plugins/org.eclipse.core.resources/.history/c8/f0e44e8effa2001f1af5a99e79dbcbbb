package com.cocea.lesson.object;

class Cow {
	public void animalSound() {//동물소리 함수를 파생
		System.out.println("음메");
	}
}

class Dog extends Cow{
	public void animalSound() {//동물소리 함수를 파생
		System.out.println("멍멍");
	}
}


