package com.cocea.lesson.tutorial;

public class Break {

	public static void main(String[] args) {
		// loop에서 break 중지
		for (int q = 0; q < 10; q++) {
			if (q == 4) {
				break;//4에서 중지
			}
			System.out.println(q);//0123
		}
	
		//continue 건너뛰고
		for (int w = 0; w < 10; w++) {
			if (w == 4) {
				continue;//4를 건너..
			}
			System.out.println(q);//0123
		}
	
	
	}

}
